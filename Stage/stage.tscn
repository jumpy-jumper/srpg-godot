[sub_resource type="GDScript" id=3]
script/source = "extends StageUI

export var fade_in_duration = 1.0

func _ready():
	visible = true
	animate()

func animate():
	$Tween.interpolate_property(self, \"modulate:a\",
		1, 0, fade_in_duration,
		Tween.TRANS_LINEAR, Tween.EASE_OUT)
	$Tween.start()
"

