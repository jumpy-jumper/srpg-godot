[sub_resource type="GDScript" id=1]
script/source = "extends Control

onready var stage = $\"../..\"

func _process(_delta):
	var enemy_count = stage.get_enemy_count()
	$Turn.text = \"T\" + str(stage.cur_tick)
	$Round.text = \"East-1\"
	$Left.text = str(enemy_count - stage.get_enemies_left()) + \"/\" + str(enemy_count)
	$Selected.rotation_degrees = 90 * stage.selected_summoner_index
"

